{
  "name": "node-arn",
  "version": "1.0.5",
  "description": "Aina Resource Naming",
  "main": "index.js",
  "scripts": {
    "test": "NODE_ENV=test jest --forceExit --detectOpenHandles --coverage --coverageDirectory=coverage"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/azanium/node-arn.git"
  },
  "keywords": [
    "arn",
    "aina",
    "ainasoft",
    "resource",
    "name",
    "naming"
  ],
  "author": "Suhendra Ahmad",
  "license": "ISC",
  "bugs": {
    "url": "https://github.com/azanium/node-arn/issues"
  },
  "homepage": "https://github.com/azanium/node-arn#readme",
  "devDependencies": {
    "eslint": "^5.9.0",
    "eslint-config-astro": "^1.0.8",
    "eslint-plugin-import": "^2.14.0",
    "jest": "^23.6.0"
  },
  "jest": {
    "verbose": true,
    "testRegex": "(/src/.*)\\.(test|spec).js$",
    "moduleFileExtensions": [
      "js"
    ],
    "modulePathIgnorePatterns": [
      "src/tests"
    ],
    "testPathIgnorePatterns": [
      "\\index.js$",
      "(/src/.*)\\.(validation|config|index).js$"
    ],
    "testEnvironment": "node",
    "collectCoverageFrom": [
      "src/**/*.{js,jsx}",
      "!src/config/**",
      "!src/**/index.js",
      "!src/database/**",
      "!src/models/**",
      "!src/**/*.validation.js",
      "!<rootDir>/node_modules/"
    ],
    "coverageReporters": [
      "json",
      "html",
      "lcov",
      "text",
      "clover"
    ]
  }
}
